{
  "mcpServers": {
    "SequentialThinking": {
      "args": [
        "-y",
        "@modelcontextprotocol/server-sequential-thinking"
      ],
      "command": "npx",
      "env": {
        "NPM_CONFIG_USERCONFIG": "~/.config/npm/npmrc"
      }
    },
    "fetch": {
      "args": [
        "mcp-server-fetch"
      ],
      "command": "uvx"
    }
  },
  "nativeMCPServers": {
    "mcphub": {
      "disabled": false
    },
    "neovim": {
      "disabled": false
    }
  }
}
